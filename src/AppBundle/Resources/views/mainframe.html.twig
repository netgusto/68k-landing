{% extends '::base.html.twig' %}

{% block title %}68K - Fédérons la communauté numérique du Haut-Rhin{% endblock %}

{% block stylesheets %}
<link rel="canonical" href="http://68k.io" />
<meta name="twitter:card" content="summary" />
<meta name="twitter:site" content="@68k_io" />
<meta property="og:url" content="http://68k.io" />
<meta property="og:title" content="68K - Fédérons la communauté numérique du Haut-Rhin" />
<meta property="og:description" content="68K regroupe les entrepreneurs et les freelances animant la scène numérique du Haut-Rhin, est entièrement gratuit, et le restera toujours." />
<meta property="og:image" content="http://68k.io/static/share-link.png" />

<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/css/materialize.min.css" />
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<meta name="google-site-verification" content="7SbdWRBEpuTZ-GBQXZbMyTvc7uKjKbqeF9cTkZLdTO8" />

<style>
{# couleur et footer collé au bas de page #}
body {
    background-color: #3B5998;
    display: flex;
    min-height: 100vh;
    flex-direction: column;
}

main {
    flex: 1 0 auto;
}

</style>
<!-- Facebook Pixel Code -->
<script>
!function(f,b,e,v,n,t,s){if(f.fbq)return;n=f.fbq=function(){n.callMethod?
n.callMethod.apply(n,arguments):n.queue.push(arguments)};if(!f._fbq)f._fbq=n;
n.push=n;n.loaded=!0;n.version='2.0';n.queue=[];t=b.createElement(e);t.async=!0;
t.src=v;s=b.getElementsByTagName(e)[0];s.parentNode.insertBefore(t,s)}(window,
document,'script','//connect.facebook.net/en_US/fbevents.js');
fbq('init', '497754073739327');
fbq('track', "PageView");</script>
<noscript><img height="1" width="1" style="display:none" src="https://www.facebook.com/tr?id=497754073739327&ev=PageView&noscript=1" /></noscript>
<!-- End Facebook Pixel Code -->
{% endblock %}

{% block javascripts %}
<script type="text/javascript" src="https://code.jquery.com/jquery-2.2.0.min.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/js/materialize.min.js"></script>
{% endblock %}


{% block body %}
    <header>
        <div class="container">
            <div class="row">
                <div class="col s8 offset-s2 m6 offset-m3 l4 offset-l4 center-align" style="margin-top: 2rem;">
                    <a href="/"><img src="/static/logo-68k-darkbg-slim.png" class="responsive-img" /></a>
                </div>
            </div>
            <div class="row white-text">
                <div class="col s8 offset-s2">
                    {% block baseline %}
                    <p class="flow-text center-align">
                        <strong>68K</strong> regroupe les entrepreneurs et les freelances animant la scène numérique du Haut-Rhin.<br /><strong>68K</strong> est entièrement gratuit, et le restera toujours.<br />
                    </p>
                    <p class="center-align"><a href="/mission" class="white-text">Lisez notre lettre de mission</a></p>
                    {% endblock %}
                </div>
            </div>
        </div>
    </header>

    <main>{% block main %}{% endblock %}</main>

    <footer class="page-footer">
        <div class="container">
            <div class="row">
                <div class="col s12 m6 l6">
                    <h5 class="white-text">Contactez-nous</h5>
                    <p class="grey-text text-lighten-4">
                        <a href="https://twitter.com/68k_io" target="_blank" class="white-text">Sur Twitter @68k_io</a><br />
                        <a href="https://www.facebook.com/reseau68K" target="_blank" class="white-text">Sur Facebook</a><br />
                        <a href="mailto:hello@68k.io" class="white-text">Par email à hello@68k.io</a><br />
                    </p>
                    <h5 class="white-text">Ressources</h5>
                    <p class="grey-text text-lighten-4">
                        <a href="https://github.com/reseau68k/68k-presse/blob/master/README.md" class="white-text">Dossier de presse</a><br />
                        <a href="https://github.com/reseau68k" class="white-text" target="_blank">Code source sur GitHub</a>
                    </p>
                </div>
                <div class="col s12 m6 l4 offset-l2">
                    <h5 class="white-text">Ils nous soutiennent</h5>
                    <ul>
                        <li><a class="grey-text text-lighten-3" href="http://netgusto.com/" target="_blank">Net Gusto</a></li>
                        <li><a class="grey-text text-lighten-3" href="http://www.publicore.net/" target="_blank">Publicore</a></li>
                        <li><a class="grey-text text-lighten-3" href="http://www.square-colmar.fr/" target="_blank">Square coworking</a></li>
                        <li><a class="grey-text text-lighten-3" href="http://www.mediascript.fr/" target="_blank"><strong>media</strong>script</a></li>
                    </ul>

                    <p><a href="/mission#soutien" class="white-text">... vous aussi, soutenez-nous !</a></p>
                </div>
            </div>
        </div>
        <div class="footer-copyright">
            <div class="container">
                © 2016 68K
            </div>
        </div>
    </footer>

    {# Google Analytics #}
<script>
(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

ga('create', 'UA-72328957-1', 'auto');
ga('send', 'pageview');

</script>


{% endblock %}
